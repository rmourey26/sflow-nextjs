{
  "buildCommand": "pnpm run build",
  "installCommand": "rm -rf node_modules .pnpm-store pnpm-lock.yaml && pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "build": {
    "env": {
      "VERCEL_FORCE_NO_BUILD_CACHE": "1",
      "NEXT_TELEMETRY_DISABLED": "1",
      "PNPM_CACHE_FOLDER": "/dev/null"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false
}
